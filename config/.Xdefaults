CLICOLOR=TRUE
LSCOLORS=Gxfxbxdxcxegedabagacad
TERM="xterm-256color"
COLORTERM="truecolor"

! special
*.foreground:   #c6c8d1
*.background:   black
*.cursorColor:  #e4e4e4

! black
*.color0:       #121212
*.color8:       #585858

! red
*.color1:       #ff0000
*.color9:       #e30000

! green
*.color2:       #00ff00
*.color10:      #1ce11c

! yellow
*.color3:       #f7ff00
*.color11:      #e4ea35

! blue
*.color4:       #003aff
*.color12:      #355eea

! magenta
*.color5:       #c900ff
*.color13:      #c900ff

! cyan
*.color6:       #47f7ff
*.color14:      #35e2ea

! white
*.color7:       #fff7e9
*.color15:      #e4e4e4

XTerm*font: xft:Hack\ Nerd\ Font:Term:style=Light:Mono:Bold:size=10:antialias=true:hinting=true
XTerm*faceName: xft:Hack\ Nerd\ Font:Term:style=Light:Mono:Bold:size=10:antialias=true:hinting=true
XTerm*faceSize: 10

XTerm.termName: xterm-256color

XTerm.vt100.locale: true
XTerm.vt100.metaSendsEscape: true

XTerm.vt100.backarrowKey: true
XTerm.ttyModes: erase ^?

XTerm.vt100.translations: #override \n\
    Ctrl <Key>M: maximize() \n\
    Ctrl <Key>R: restore()

XTerm.vt100.saveLines: 215096

XTerm.vt100.scrollBar: true
XTerm.vt100.scrollbar.width: 8

XTerm.vt100.geometry: 80x32

XTerm.vt100.selectToClipboard

XTerm.vt100.reverseVideo: false








